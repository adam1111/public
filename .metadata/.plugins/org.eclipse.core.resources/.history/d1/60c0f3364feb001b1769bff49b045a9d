package CreatnalPatterns;

public class mains 
{
	public static void main(String[] arg)
	{
		Thread s = new Thread();
		System.out.println(s.isAlive());
		
		Countter x = Countter.getCounter();
		x.AddOne();
		System.out.println(x.counter);
		
		Countter y = Countter.getCounter();
		y.AddOne();
		
		System.out.println(y.counter);
		
	}

}
